  <xs:complexType name="colourSpecificationBoxType">
    <xs:sequence>
      <xs:element type="xs:string" name="meth"/>
      <xs:element type="xs:string" name="prec"/>
      <xs:element type="xs:string" name="approx"/>
      <xs:element type="xs:string" name="enumCS"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>