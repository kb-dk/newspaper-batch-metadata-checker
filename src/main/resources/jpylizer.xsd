  <xs:complexType name="comType">
    <xs:sequence>
      <xs:element name="lcom">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="15"/>
            <xs:enumeration value="347"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element type="xs:string" name="rcom"/>
      <xs:element name="comment">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Kakadu-v7.2"/>
            <xs:enumeration value="Kdu-Layer-Info: log_2{Delta-D(squared-error)/Delta-L(bytes)}, L(bytes)*-175.6, 3.6e+006*-176.8,
                    3.6e+006*-177.9, 3.6e+006*-179.1, 3.6e+006*-180.3, 3.6e+006*-181.5, 3.7e+006*-182.6,
                    3.7e+006*-183.8, 3.7e+006*-185.0, 3.7e+006*-186.1, 3.7e+006*-187.3, 3.7e+006*-188.5,
                    3.7e+006*-189.7, 3.7e+006*-190.8, 3.7e+006*-192.0, 3.7e+006*-192.0, 6.6e+006*
                "/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
